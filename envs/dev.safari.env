# Development environment for Safari (SQLite)
DEBUG=True
DJANGO_ENV=development
DJANGO_SETTINGS_MODULE=config.settings.dev

# Database configuration
DATABASE_ENGINE=django.db.backends.sqlite3
DATABASE_NAME=db.sqlite3
DATABASE_USER=
DATABASE_PASSWORD=
DATABASE_HOST=
DATABASE_PORT=

# Server configuration
WSGI_SERVER=runserver
WSGI_PORT=8765

# Security settings - all SSL/HTTPS disabled
SECRET_KEY=django-insecure-safari-key
ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0
SECURE_SSL_REDIRECT=False

# Static files
STATIC_URL=/static/
MEDIA_URL=/media/
STATICFILES_STORAGE=django.contrib.staticfiles.storage.StaticFilesStorage

# AWS Bedrock configuration
AWS_ACCESS_KEY_ID=your-access-key-id-here
AWS_SECRET_ACCESS_KEY=your-secret-access-key-here
AWS_REGION=us-east-1
AWS_BEDROCK_PROMPT_ARN=arn:aws:bedrock:us-east-1:891377295311:prompt/LGO4BMQJG7
